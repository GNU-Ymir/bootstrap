in pattern_match;

use ymirc::utils::_;
use utils;


__test {
    for i in 1 ... 25 {
        logging::setRelativeTime ();
        logging::info (" Test pattern match ", i, " start");
        utils::registerTest ("test_resources/pattern_match/test" ~ std::conv::to!{[c8]} (i) ~ ".yr");
        logging::success (" Test pattern match ", i, " finished");
    }
}
