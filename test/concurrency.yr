in concurrency;

use ymirc::utils::_;
use utils;

__test {
    logging::setRelativeTime ();
    logging::info ("Test concurrency 1 start");

    let res = utils::compileFile ("test_resources/concurrency/test1.yr", withLint-> true);
    utils::ensureErrorEmpty (res.err);
    utils::compareResultWithFile (res.sem, "test_resources/concurrency/test1_out.txt");
    utils::compareResultWithFile (res.exp, "test_resources/concurrency/test1_yil.txt");

    logging::success ("Test concurrency 1 finished");
}
