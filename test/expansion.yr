in expansion;

use ymirc::utils::_;
use utils;


__test {
    logging::setRelativeTime ();
    logging::info (" Test expansion 1 start");

    let res = utils::compileFile ("test_resources/expansion/test1.yr", withLint-> true);
    utils::ensureErrorEmpty (res.err);
    utils::compareResultWithFile (res.sem, "test_resources/expansion/test1_out.txt");
    utils::compareResultWithFile (res.exp, "test_resources/expansion/test1_yil.txt");

    logging::success (" Test expansion 1 finished");
}

