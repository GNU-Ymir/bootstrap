in class_ctors;

use ymirc::utils::_;
use utils;

__test {
    logging::setRelativeTime ();
    logging::info (" Test class ctors 1 start");

    let res = utils::compileFile ("test_resources/lit_class/ctors/test1.yr", withLint-> true);
    utils::ensureErrorEmpty (res.err);
    utils::compareResultWithFile (res.sem, "test_resources/lit_class/ctors/test1_out.txt");
    utils::compareResultWithFile (res.exp, "test_resources/lit_class/ctors/test1_yil.txt");

    logging::success (" Test class ctors finished");
}

