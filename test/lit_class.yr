in lit_class;

use ymirc::utils::_;
use utils;


__test {
    for i in 1 ... 7 {
        logging::setRelativeTime ();
        logging::info (" Test lit class ", i, " start");
        utils::registerTest ("test_resources/lit_class/test" ~ std::conv::to!{[c8]} (i) ~ ".yr");
        logging::success (" Test lit class ", i, " finished");
    }
}

mod errors {
    __test {
        for i in 1 ... 10 {
            logging::setRelativeTime ();
            logging::info (" Test lit class err ", i, " start");
            utils::registerTest ("test_resources/lit_class/errors/test" ~ std::conv::to!{[c8]} (i) ~ ".yr");
            logging::success (" Test lit class err ", i, " finished");
        }
    }
}
