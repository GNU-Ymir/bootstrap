in binary_int;

use ymirc::utils::_;
use utils;
use std::io;

// Test for binary operators on ints
__test {
    for i in 1 ... 7 {
        logging::setRelativeTime ();
        logging::info (" Test binary int ", i, " start");
        utils::registerTest ("test_resources/binary_int/test" ~ std::conv::to!{[c8]} (i) ~ ".yr");
        logging::success (" Test binary int ", i, " finished");
    }
}
