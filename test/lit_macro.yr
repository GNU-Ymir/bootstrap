in lit_macro;


use ymirc::utils::_;
use utils;

__test {
    for i in 1 ... 1 {
        logging::setRelativeTime ();
        logging::info (" Test lit macro ", i, " start");
        utils::registerTest ("test_resources/lit_macro/test" ~ std::conv::to!{[c8]} (i) ~ ".yr");
        logging::success (" Test lit macro ", i, " finished");
    }
}
