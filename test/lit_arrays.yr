__test {
    logging::setRelativeTime ();
    logging::info (" Test lit arrays 1 start"s8);


    let (gens, errors) = utils::compileFile ("test_resources/lit_arrays/test1.yr"s8);
    utils::ensureErrorEmpty (errors);
    utils::compareResultWithFile (gens, "test_resources/lit_arrays/test1_out.txt"s8);

    logging::success (" Test lit arrays 1 finished"s8);
}

