in templates;

use ymirc::utils::_;
use utils;

__test {
    for i in 1 ... 18 {
        logging::setRelativeTime ();
        logging::info (" Test template ", i, " start");
        utils::registerTest ("test_resources/templates/test" ~ std::conv::to!{[c8]} (i) ~ ".yr");
        logging::success (" Test template ", i, " end");
    }
}
