in binary_float;

use ymirc::utils::_;
use utils;
use std::io;

// Test for binary operators on floats
__test {
    logging::setRelativeTime ();
    logging::info (" Test binary float 1 start");

    let res = utils::compileFile ("test_resources/binary_float/test1.yr");
    utils::ensureErrorEmpty (res.err);
    utils::compareResultWithFile (res.sem, "test_resources/binary_float/test1_out.txt");

    logging::success (" Test binary float 1 finished");
}

