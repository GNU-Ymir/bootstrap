in base;

use ymirc::lint::node::{base, type::base};
use ymirc::lexing::word;

/**
 * A Ymir Intermediate Language Value
 * */
@abstract
pub class YILValue over YILNode {

    let _type : &YILType;

    pub self (loc : &Word, type : &YILType)
        with super (loc)
        , _type = type
    {}

    @final
    pub fn getType (self)-> &YILType {
        self._type
    }

}
