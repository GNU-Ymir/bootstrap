/**
 * List of suffixes that can be found right after a string token
 * */
pub enum
| UTF8  = "s8"
| UTF16 = "s16"
| UTF32 = "s32"
 -> StringSuffixes;

/**
 * List of suffixes that can be found right after a char token
 * */
pub enum
| UTF8  = "c8"
| UTF16 = "c16"
| UTF32 = "c32"
 -> CharSuffixes;

/**
 * List of suffixes that can be found right after a numeric value
 * */
pub enum
| U8	= "u8"
| U16	= "u16"
| U32	= "u32"
| U64	= "u64"
| USIZE = "us"
| I8	= "i8"
| I16	= "i16"
| I32	= "i32"
| I64	= "i64"
| ISIZE = "is"
 -> FixedSuffixes;

/**
 * List of suffixes that can be found right after a float value
 * */
pub enum
| F32   = "f"
| F64   = "d"
| F80   = "l"
| REAL  = "r"
 -> FloatSuffixes;

/**
 * List of exponent string
 * */
pub enum
| LE = "e"
| BE = "E"
| LP = "p"
| BP = "P"
 -> FloatExp;


