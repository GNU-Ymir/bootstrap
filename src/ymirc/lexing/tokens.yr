in tokens;

pub lazy TokenList = copy Tokens::__members__;
pub lazy SkipList = copy SkipTokens::__members__;
pub lazy CommentList = copy CommentTokens::__members__;

pub fn isMultipleLineComment (open : [c8])-> bool {
    open.len >= 2 && open [0 .. 2] == "/*"
}

pub enum
| MULTILINE        = ("/*", "*/")
| MULTILINE_NO_DOC = ("/*!", "*/")
| ONELINE          = ("//", "\n")
 -> CommentTokens;

