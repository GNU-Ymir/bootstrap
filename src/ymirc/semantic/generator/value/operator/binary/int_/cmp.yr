in cmp;

use ymirc::lexing::word;
use ymirc::syntax::keys;
use ymirc::utils::format;
use std::stream;

use ymirc::semantic::generator::value::operator::binary::base;
use ymirc::semantic::generator::{base,
                                 type::base,
                                 value::base,
                                 type::native::scalar::bool_};

/**
 * Comparison operator on two int values
 */
@final
pub class CmpIntOperatorValue over BinaryOperatorValue {

    pub self (loc : &Word, op : BinaryOperators, left : &Value, right : &Value, isReduced : bool = false)
        with super (loc, copy BoolType (loc), op, left, right, isReduced-> isReduced)
    {}

    pub over clone (self)-> &Value {
        copy CmpIntOperatorValue (self._loc, self._op,
                                  self._left.clone (),
                                  self._right.clone (),
                                  isReduced-> self._isReduced)
    }

    impl Streamable;
}
