in cmp;

use ymirc::lexing::word;
use ymirc::lexing::keys;
use ymirc::utils::format;
use std::stream;
use ymirc::semantic::generator::type::native::scalar::bool_;

/**
 * Binary comparison operator on two char values
 */
@final
pub class CmpCharOperatorValue over BinaryOperatorValue {

    pub self (loc : &Word, operator : BinaryOperators, left : &Value, right : &Value, isReduced : bool = false)
        with super (loc, copy BoolType (loc), operator, left, right, isReduced-> isReduced)
    {}

    pub over clone (self)-> &Value {
        copy CmpCharOperatorValue (self._loc,
                                   self._op,
                                   self._left.clone (),
                                   self._right.clone (),
                                   isReduced-> self._isReduced)
    }

    impl Streamable;
}
