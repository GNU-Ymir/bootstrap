in empty;

use ymirc::utils::format;
use ymirc::lexing::word;
use std::stream;

pub lazy EMPTY_GENERATOR = copy EmptyGenerator ();

@final
pub class  EmptyGenerator over Generator {

    pub self ()
        with super (EOF_WORD)
    {}

    /*!
     * ================================================================================
     * ================================================================================
     * =========================          COMPARISON          =========================
     * ================================================================================
     * ================================================================================
     */

    pub over opEquals (self, o : &Generator)-> bool {
        match o {
            EmptyGenerator () => { true }
            _ => { false }
        }
    }    
    
    /*!
     * ================================================================================
     * ================================================================================
     * =========================        MISCELLANEOUS         =========================
     * ================================================================================
     * ================================================================================
     */

    impl Streamable;
    impl Formattable {
        pub over format (self, dmut stream : &Formatter) {
            stream:.write ("<empty-generator>");
        }
    }
}
