in operator;

pub mod ::binary;
pub mod ::mult;
pub mod ::path;
pub mod ::unary;

use ymirc::syntax::keys;
use ymirc::lexing::{word, tokens};
use ymirc::utils::format;

/**
 * The ancestor of all kind of operator
 */
@abstract
pub class OperatorExpr over Expression {

    pub self (loc : &Word)
        with super (loc)
    {}

    impl Streamable;
    impl Formattable {
        pub over format (self, dmut _ : &Formatter) {}
    }
}
