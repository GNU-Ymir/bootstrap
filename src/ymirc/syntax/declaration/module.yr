/*!
 * ====================================================================================================
 * ====================================================================================================
 * ====================================          SUBMODS          =====================================
 * ====================================================================================================
 * ====================================================================================================
 */

mod ::assert_;
mod ::def_;
mod ::global;
mod ::import;
mod ::root;
mod ::template;
mod ::use_;

/*!
 * ====================================================================================================
 * ====================================================================================================
 * =====================================          TYPES          ======================================
 * ====================================================================================================
 * ====================================================================================================
 */


pub def AssertDecl:    assert_::AssertDecl;
pub def DefDecl:       def_::DefDecl;
pub def GlobalVarDecl: global::GlobalVarDecl;
pub def ImportDecl:    import::ImportDecl;
pub def ModuleDecl:    root::ModuleDecl;
pub def TemplateDecl:  template::TemplateDecl;
pub def UseDecl:       use_::UseDecl;
