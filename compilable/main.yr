import std::io;

class A {

    pub let a : i32 = 8;
    
    pub def foo (self) {
        println (self.a);
    }

    pub def bar (mut self, x : i32) {
        println (self.a, " ", x);
    }
    
    pub self () {}
}


def main () {
    let dmut a = A::new ();
    a.foo ();
    a.bar (12);
}
