@abstract
class X {
    pub self () {}

    pub fn foo (mut self);
    pub fn bar (self);
}

class Y over X {
    let b : bool;
    pub self (b : bool)
        with b = b
    {}

    pub over foo (mut self) {
        if !self.b {
            self.super:.foo ();
        }
    }

    pub over bar (self) {
        if !self.b {
            self.super.bar ();
        }
    }
}
