@abstract
class A {
    let _x : i32 = 0;
    let _y : i32 = 9;

    pub self () {}

    @field
    pub fn foo (self)-> i32;
}

class B over A {
    pub self () {}

    pub over foo (self)-> i32 {
        self._x
    }
}

fn main () {
    let b : &A = copy B ();
    let _z_ = b.foo;
    let _y_ = b.foo (1);
}
