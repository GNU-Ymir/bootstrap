record A {
    pub self () {}
}

class B {
    pub self () {}
}

entity C {
    pub self () {}
}

fn foo () {
    let a = A ();
    a.__typeid__;
    a.__typeinfo;
    a.__typeinfo__;
}

fn bar () {
    let b = copy B ();
    b.__typeid__;
    b.__typeinfo;
    b.__typeinfo__;
}

fn baz () {
    let c = C ();
    c.__typeid__;
    c.__typeinfo;
    c.__typeinfo__;
}
