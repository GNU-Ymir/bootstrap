@abstract
class A {
    pub self () {
        self.foo ();
    }

    pub fn foo (self);
}


class B over A {
    let x : i32;
    pub self ()
        with x = 10
    {}

    pub over foo (self) {
        if self.x != 10 {
            panic;
        }
    }
}


fn main () {
    let _b_ = copy B ();
}
