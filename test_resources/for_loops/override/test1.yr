class A {
    let _items = copy [1, 2, 3];

    pub self () {}

    pub fn begin (self)-> dmut &Iterator {
        Iterator::new (0, self)
    }

    pub fn end (self)-> usize {
        self._items.len
    }

    @field
    pub fn values (self)-> [i32] {
        self._items
    }

}


class Iterator {

    let _x : &A;
    let mut _index : usize;


    pub self (index : usize, x : &A)
        with _index = index
        , _x = x
    {}

    pub fn get {0us, 1us} (self)-> i32 {
        self._x.values [self._index]
    }

    pub fn next (mut self)-> dmut &Iterator {
        self._index += 1;
        alias self
    }

    pub fn opEquals (self, o : usize)-> bool {
        self._index == o
    }

}


fn main () {
    let a = A::new ();
    a.values;
    for i in a {
        std::io::println (i);
    }
}
