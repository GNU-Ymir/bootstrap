class A {N : usize, T...} {
    let a : (T,);

    pub self (a : (T,)) with a = a {
        ;
    }

    pub fn foo (self)-> (T,) {
        self.a
    }
}


fn foo {T...} (a : &A!{2us, i32, T})-> (i32, T) {
    a.foo ()
}


fn main () {
    let a = A!{2us, i32, i32, i32}::new ((12,11,10));
    foo (a);
}
