
class A {
    pub self () {}
}

class B over A {
    pub let z : i32 = 9;
    pub self () {}

    @field
    pub fn foo (self)-> i32 {
        12
    }

}

fn baz (_ : [c8], _ : i32) {}
fn main () {
    let b : &A = copy B ();
    match b {
        B (foo-> 12, z-> x : _) => {
            baz ("B with foo = 12, z = ", x);
        }
    }
}
