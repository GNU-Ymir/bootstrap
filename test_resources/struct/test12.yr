struct X {
    pub let a : i32;
    pub let b : i32;

    pub self (a : i32, b : i32)
        with a = a
        , b = b
    {}
}

fn foo (a : i32) {
    a;
}

fn main () {
    let a = X::init (1, 2);
    cte for i in 0us .. X::field_infos.len {
        foo (__pragma!field (a, cte {X::field_infos [i].name}));
    }
}
