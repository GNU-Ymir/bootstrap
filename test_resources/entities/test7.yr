fn foo () {}

entity B {
    pub let mut i : i32 = 0;
    pub self () {}
    pub self (i : i32)
        with i = i {}

    __dtor (mut self) {
        foo ();
    }
}

entity A {
    pub let dmut z : B  = B ();

    pub self () {}
    pub self (i : i32)
        with z = B (i)
    {}

    pub fn change (mut self) {
        self.z = B (83);
    }

    __dtor (mut self) {
        foo ();
    }
}


fn main () {
    let dmut a = A (12);
    a = A (45);
    a:.change ();
}
